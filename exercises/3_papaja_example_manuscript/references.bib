  keywords = {consciousness,Implicit learning,Fringe consciousness,Personality,Serial reaction time task,Consciousness},
  author = {Norman, Elisabeth and Price, Mark C. and Duff, Simon C.},
  file = {/home/mariusbarth/Zotero/storage/3PA46Z9G/Norman et al. - 2006 - Fringe consciousness in sequence learning The inf.pdf}
}

@article{fu_can_2010,
  title = {Can {{Unconscious Knowledge Allow Control}} in {{Sequence Learning}}?},
  volume = {19},
  issn = {1053-8100},
  url = {http://www.sciencedirect.com/science/article/pii/S1053810009001536},
  doi = {10.1016/j.concog.2009.10.001},
  abstract = {This paper investigates the conscious status of both the knowledge that an item is legal (judgment knowledge) and the knowledge of why it is legal (structural knowledge) in sequence learning. We compared ability to control use of knowledge (Process Dissociation Procedure) with stated awareness of the knowledge (subjective measures) as measures of the conscious status of knowledge. Experiment 1 showed that when people could control use of judgment knowledge they were indeed conscious of having that knowledge according to their own statements. Yet Experiment 2 showed that people could exert such control over the use of judgment knowledge when claiming they had no structural knowledge: i.e. conscious judgment knowledge could be based on unconscious structural knowledge. Further implicit learning research should be clear over whether judgment or structural knowledge is claimed to be unconscious as the two dissociate in sequence learning.},
  number = {1},
  journaltitle = {Consciousness and Cognition},
